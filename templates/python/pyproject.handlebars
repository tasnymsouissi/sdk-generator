[tool.poetry]
name = "{{projectName}}"
version = "{{packageVersion}}"
description = "{{appName}}"
authors = ["{{#if infoName}}{{infoName}}{{/if}}{{#unless infoName}}OpenAPI Generator community{{/unless}} <{{#if infoEmail}}{{infoEmail}}{{/if}}{{#unless infoEmail}}team@openapitools.org{{/unless}}>"]
packages = [
    {include = "{{packageName}}"}
]

[tool.poetry.dependencies]
python = "^3.9"
setuptools = "^21.0.0"
certifi = "^2022.9.24"
python-dateutil = "^2.5.3"
urllib3 = "^1.25.3"
{{#if asyncio}}
aiohttp = "^3.0.0"
{{/if}}
{{#if tornado}}
tornado = "^4.2,<5"
{{/if}}
{{#if hasHttpSignatureMethods}}
pem = "^19.3.0"
pycryptodome = "^3.9.0"
{{/if}}
typing-extensions = "^4.4.0"
backmarket-auth-s2s-jwe = { version = "^0.1.3", optional = true }

[tool.poetry.dev-dependencies]

[build-system]
requires = ["poetry-core>=1.0.0"]
build-backend = "poetry.core.masonry.api"

[tool.poetry.extras]
auth-s2s-jwe = ["backmarket-auth-s2s-jwe"]
